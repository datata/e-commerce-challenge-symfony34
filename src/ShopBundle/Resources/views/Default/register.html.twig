{% extends 'base.html.twig' %}
{% block body %}
   
<div class="container">

  <div class="row justify-content-md-center">
    <div class="col-lg-6">


{{ form_start(form) }}
    
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.plainPassword.first) }}
    {{ form_row(form.plainPassword.second) }}

    <button type="submit">Register!</button>
{{ form_end(form) }}

    </div>
      </div>
    </div>


{% endblock %}
